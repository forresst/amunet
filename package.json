{
	"name": "amunet",
	"version": "1.0.0",
	"description": "Amunet : markdown metadata hidden",
	"license": "MIT",
	"type": "module",
	"exports": {
		"types": "./dist/index.d.ts",
		"default": "./dist/index.js"
	},
	"engines": {
		"node": ">=18"
	},
	"files": [
		"dist/*",
		"!dist/test"
	],
	"types": "dist/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/forresst/amunet.git"
	},
	"bugs": {
		"url": "https://github.com/forresst/amunet/issues"
	},
	"homepage": "https://github.com/forresst/amunet#readme",
	"author": {
		"name": "St√©phane Reynaud",
		"url": "https://github.com/forresst"
	},
	"scripts": {
		"test": "npm run build && xo && c8 ava",
		"build": "del-cli dist && tsc",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"amunet",
		"markdown",
		"metadata",
		"hidden"
	],
	"dependencies": {
		"make-dir": "^3.0.2"
	},
	"devDependencies": {
		"@ava/typescript": "^5.0.0",
		"@sindresorhus/tsconfig": "^7.0.0",
		"@types/mock-fs": "^4.13.4",
		"ava": "^6.2.0",
		"c8": "^10.1.3",
		"del-cli": "^6.0.0",
		"mock-fs": "^5.4.1",
		"typescript": "^5.7.2",
		"xo": "^0.60.0"
	},
	"c8": {
		"include": [
			"dist/*.js"
		],
		"reporter": [
			"lcov",
			"text"
		]
	},
	"ava": {
		"typescript": {
			"rewritePaths": {
				"source/": "dist/"
			},
			"compile": false
		}
	}
}
